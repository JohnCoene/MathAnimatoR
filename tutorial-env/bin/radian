#!/home/david/requests/MathAnimatoR/tutorial-env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'radian==0.6.6','console_scripts','radian'
__requires__ = 'radian==0.6.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('radian==0.6.6', 'console_scripts', 'radian')()
    )
